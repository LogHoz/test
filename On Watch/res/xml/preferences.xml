                android:summary="@string/prefs_debug_time_summary" />
 
        <CheckBoxPreference
                android:key="debugSpace"
                android:title="@string/prefs_debug_space"
                android:summary="@string/prefs_debug_space_summary" />
            
    </PreferenceCategory>
     
</PreferenceScreen>

